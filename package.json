{
  "name": "soo1-loglens",
  "version": "0.0.11",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.es.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.umd.js"
      }
    },
    "./babel-plugin": {
      "import": "./babel/index.js",
      "default": "./babel/index.js"
    },
    "./vite": {
      "import": {
        "types": "./unplugin/index.d.ts",
        "default": "./unplugin/index.js"
      }
    },
    "./webpack": {
      "import": {
        "types": "./unplugin/index.d.ts",
        "default": "./unplugin/index.js"
      }
    },
    "./rollup": {
      "import": {
        "types": "./unplugin/index.d.ts",
        "default": "./unplugin/index.js"
      }
    }
  },
  "files": [
    "dist",
    "babel",
    "unplugin"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run build:unplugin",
    "build:unplugin": "tsc src/unplugin/index.ts --outDir unplugin --declaration --skipLibCheck",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "dependencies": {
    "unplugin": "^1.14.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/babel__core": "^7.20.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ts-jest": "^29.4.5",
    "typescript": "~5.9.3",
    "vite": "^7.1.7",
    "vite-plugin-dts": "^4.5.4"
  }
}
